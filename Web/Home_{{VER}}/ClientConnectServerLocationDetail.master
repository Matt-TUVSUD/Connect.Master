    <script type="text/javascript">
        // GLOBAL VARIABLES USED IN INCLUDED JS FILES
        var global_IsMainGaugePage = false;
        var global_PracticeCode = "";
        var global_IsAdminWindowOpen = false;
        var global_EnableAdminWindow = true;

        $(document).ready(function () {
            showTabAdmin("<%= ServerSide_AdminTab %>");
        
        //$("#include_AdminWindow").load("../Common/AdminWindow.html");
    });

    // ADMIN WINDOW
    var windowAdmin = $("#windowAdmin");
    windowAdmin.kendoWindow({
        width: "725px",
        height: "500px",
        title: "", // title added below with JQuery
        actions: ["close"],
        modal: true,
        resizable: false,
        moveable: false,
        close: onAdminWindowClose,
        open: onAdminWindowOpen
    });
    //$("#windowAdmin").closest(".k-window").css({
    //    top: 145,
    //    left: 215
    //});
    $("#windowAdmin").prev().find(".k-window-title").append("<img src='../images/icons/admin/businesswoman_24x24.png' align='left' alt='Administration Window' class='windowIcon'/>  " + "Administration Window");

    function onAdminClick() {
        if (global_EnableAdminWindow) {
            windowAdmin.data("kendoWindow").center().open();
            $("#windowAdmin").css("display", "block");
            global_IsAdminWindowOpen = true;
        }
    }

    function onAdminWindowClose() {
        refreshTabs();
        refreshPage();
        windowAdminTabStrip.select(0);
        global_IsAdminWindowOpen = false;
    }

    function onAdminWindowOpen() {
        if (windowAdminUpload == null) {
            setupAdminKendo();
        }
        viewAdmin.onLoadDocManagerTab();
        windowAdminTabStrip.select(0);
        global_IsAdminWindowOpen = true;
    }

    // DEFINE PLEASE WAIT WINDOW
    var windowPleaseWait = $("#windowPleaseWait");
    windowPleaseWait.kendoWindow({
        width: "300px",
        height: "220px",
        title: "", // title added below with JQuery
        actions: {},
        animation: { open: { effects: "default" }, close: { effects: "default" } },
        modal: true
    });
    $("#windowPleaseWait").closest(".k-window").css({
        top: 300,
        left: 375
    });
    $("#windowPleaseWait").prev().find(".k-window-title").append("<img src='../images/icons/wait/icon_hourglass_32x32.png' align='left' alt='Please Wait' class='windowIcon'/>  " + "Please Wait");

    function cancelProgramStatus() {
        $("#windowPleaseWaitProcessed").hide();
        $("#windowPleaseWaitCancelled").show();
        var gSafeGuid = getGSafeGUID();
        document.location.href = "../Gauge/Default.aspx?sid=" + gSafeGuid;
    }

    function refreshTabs() {
        // DON'T DO THE BELOW, OUT OF MEMORY ERROR AND BAD RESULTS
        viewAdmin.onCancelReportPicker();
        viewAdmin.onCancelDiagramPicker();
        viewAdmin.onClearDocManager();
        viewAdmin.onUploadCancel();
        viewAdmin.onClearRecResponse();
    }

