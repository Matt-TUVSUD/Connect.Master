<!-- FOR KENDO GRID ROW DETAIL TEMPLATES -->
<!-- CELL DATA REPLACERS USE #= NameOfField # -->
<!-- AVOID USING # AS A CHARACTER IN THE TEMPLATE -->
<!-- IF USING A CLASS LOOKUP TO THE GRID, MAKE SURE THAT CLASS IS IN THE COLUMN TEMPLATE -->
<!-- SCRIPTS INCLUDED HERE GET RUN EVERY TIME THE DETAIL POPUP IS OPENED -->
<!-- WHEN COPIED TO THE DATABASE, THE HTML AND JS MUST BE ALL IN ONE LINE -->
<!-- DO NOT USE DOUBLE QUOTES IN JS OR HTML -->
<!-- DO NOT USE COMMENTS IN JS, ALL ONE LINE COMMENTS WILL NOT RUN CODE -->

<!-- FormViewNameOfGrid LAYOUT -->
<div id='divDetailForm_#= uid #' class='divDetailForm'>
    
    <div class='LDHead'>
        Location Detail</div>
    <!--Row 1-->
    <div class='LDInfo'>
        <div>
            <span id='lblFileNo'>File No.</span>
            <input id='fileno' readonly='readonly' type='text' value='#= fileno #' />
            <span id='lblClientLocID'>Client Loc. ID</span>
            <input id='clientlocno' readonly='readonly' type='text' value='#= clientlocno #' />
        </div>
        <!--Row 2-->
        <div>
            <span id='lblAddress'>Address</span>
            <input id='address' readonly='readonly' type='text' value='#= address #' />
            <span id='lblCity'>City</span>
            <input id='city' readonly='readonly' type='text' value='#= city #' />
            <span id='lblStProv'>St/Prov</span>
            <input id='stprov' readonly='readonly' type='text' value='#= stprov #' />
            <span id='lblCountry'>Country</span>
            <input id='country' readonly='readonly' type='text' value='#= country #' />
        </div>
        <!--Row 3-->
        <div>
            <span id='lblDivision'>Division</span>
            <input id='division' readonly='readonly' type='text' value='#= division #' />
            <span id='lblFacility'>Facility</span>
            <input id='facility' readonly='readonly' type='text' value='#= facility #' />
        </div>
    </div>
    <div class='FVTIV'>
        <div id='lblTIV'>TIV</div>
        <div>(millions)</div>
        <div>
            <input id='TIV' readonly='readonly' type='text' value='#= Tiv #' />
        </div>
    </div>
    <div class='FVTIV'>
        <div>Date Of</div>
        <div>Inspection</div>
        <div>
            <input id='SurveyDateString' readonly='readonly' type='text' value='#= SurveyDateString #' />
        </div>
    </div>
    <div class='FVTIV'>
        <div>HE</div>
        <div>Rating</div>
        <div>
            <input id='humanelementrating' readonly='readonly' type='text' value='#= humanelementrating #' />
        </div>
    </div>
    <div class='FVTable'>
        <table cellspacing='0' cellpadding='2'>
            <tr class='th'>
                <td colspan='7' class='rb'>
                    Inspection/Testing
                </td>
                <td colspan='7' class='b'>
                    Management
                </td>
            </tr>
            <tr class='tch'>
                <td class='rb'>
                    Sprinker<br />
                    Valves
                </td>
                <td class='rb'>
                    Fire<br />
                    Pumps
                </td>
                <td class='rb'>
                    Alarms
                </td>
                <td class='rb'>
                    Exting-<br />
                    uishers
                </td>
                <td class='rb'>
                    Fire<br />
                    Doors
                </td>
                <td class='rb'>
                    Comb.<br />
                    Safety
                </td>
                <td class='hrb'>
                    Infrared<br />
                    Scans
                </td>
                <td class='rb'>
                    Hot<br />
                    Work
                </td>
                <td class='rb'>
                    Contr.<br />
                    Supv
                </td>
                <td class='rb'>
                    Imp.<br />
                    Handling
                </td>
                <td class='rb'>
                    House-<br />
                    keeping
                </td>
                <td class='rb'>
                    Gen.<br />
                    Maint.
                </td>
                <td class='rb'>
                    Smoking<br />
                    Controls
                </td>
                <td class='b'>
                    Mgmt. of<br />
                    Change
                </td>
            </tr>
            <tr class='tch'>
                <td class='r'>
                    <input style='width: 50px' id='sprinklervalves' readonly='readonly' type='text' value='#= sprinklervalves #' />
                </td>
                <td class='r'>
                    <input style='width: 64px' id='firepumps' readonly='readonly' type='text' value='#= firepumps #' />
                </td>
                <td class='r'>
                    <input style='width: 50px' id='alarms' readonly='readonly' type='text' value='#= alarms #' />
                </td>
                <td class='r'>
                    <input style='width: 50px' id='extinguishers' readonly='readonly' type='text' value='#= extinguishers #' />
                </td>
                <td class='r'>
                    <input style='width: 64px' id='firedoors' readonly='readonly' type='text' value='#= firedoors #' />
                </td>
                <td class='r'>
                    <input style='width: 50px' id='combustionsafetycontrols' readonly='readonly' type='text' value='#= combustionsafetycontrols #' />
                </td>
                <td class='hr'>
                    <input style='width: 50px' id='infraredscans' readonly='readonly' type='text' value='#= infraredscans #' />
                </td>
                <td class='r'>
                    <input style='width: 50px' id='hotworkmonitoring' readonly='readonly' type='text' value='#= hotworkmonitoring #' />
