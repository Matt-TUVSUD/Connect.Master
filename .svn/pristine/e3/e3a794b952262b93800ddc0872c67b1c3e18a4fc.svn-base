<asp:Content ID="ClientConnectTestBodyContent" ContentPlaceHolderID="ClientConnectFormBody" runat="server">
    <form id="form1" runat="server">
        <!--HEADER-->
        <header>
            <grcRSC:Header ID="header" runat="server" />
