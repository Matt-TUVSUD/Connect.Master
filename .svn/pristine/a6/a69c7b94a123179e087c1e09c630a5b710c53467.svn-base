/*** FULL CONTAINER ***/
.gaugeContainer
{
    width: 800px;
    height: 204px; 
}


/*** LEFT SECTION ***/
.gaugeLeftContainer
{
    width: 140px;
    height: 204px;
    float: left;
}


/*** MIDDLE SECTION ***/
.gaugeMiddleContainer
{
    width: 132px;
    height: 204px;
    float: left;
    background-image: url('../../Home_{{VER}}/Images/gauge/gauge_English/innerGauge_none/img_gauge_middle_map.png');
    background-position: left 37px;
    background-repeat: no-repeat;
}
.gaugeMiddleTop
{
    height: 37px;
}
.gaugeMiddleMap
{
    height: 123px;
}
.gaugeMiddleBottom
{
    height: 44px;
}


/*** RIGHT SECTION AND BUFFER ***/
.gaugeRightContainer
{
    width: 503px;
    height: 204px;
    float: left;
    background-image: url('../../Home_{{VER}}/Images/gauge/gauge_English/innerGauge_none/img_gauge_right_banner.png');
    background-position: center;
    background-repeat: no-repeat;
}
.gaugeRightTop
{
    height: 44px;
}
.gaugeRightBanner
{
    height: 116px;
}
.gaugeRightBottom
{
    height: 44px;
    background-image: url('../../Home_{{VER}}/Images/gauge/GaugeShell/img_gauge_right_bottom.png');
}
.gaugeRightBottomEnglish
{
    height: 44px;
    background-image: url('../../Home_{{VER}}/Images/gauge/GaugeShell/img_gauge_right_bottom.png');
}
