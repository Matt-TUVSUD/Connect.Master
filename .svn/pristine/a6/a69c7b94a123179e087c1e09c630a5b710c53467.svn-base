/*** IMAGE STYLES ***/
.dialImageBackground
{
    border: 0px;
}
.dialImage
{
    border: 0px;
    display: none;
}
.bannerImageBackground
{
    border: 0px;
}
.bannerImage
{
    border: 0px;
    display: none;
}


/*** QUICK INFO STYLES ***/
.gaugeStatsContainer
{
    clear: left;
    font-family: Segoe UI, Arial, sans-serif;
    /*font-size: 11pt;*/
    font-size: 9pt;    
    width: 498px;
    padding-left: 310px;
    position: absolute;
    line-height: normal !important;
    border: 0;
    display: none;
}
.gaugeStatsContainer span
{
    line-height: normal !important;
    border: 0 !important;
    margin: 0 !important;
    padding: 0 !important;
}
.gaugeStatsLine
{
    width: 250px;
}
.gaugeStatsLabel
{
    width: 250px;
}
.gaugeStatsNumber
{
}
.gaugeStatsLink
{
    cursor: pointer;
}

.gaugeStatsContainerNatHaz
{
    float: left;
    font-family: Segoe UI, Arial, sans-serif;
    /*font-size: 11pt;*/
    font-size: 9pt;    
    border-collapse: collapse;
    padding: 0px;
    margin: 0px;
    /*width: 792px;*/
    width: 900px;
    padding-left: 15px;
    line-height: normal;
    border: 0;
    display: none;
}
.gaugeStatsContainerNatHaz span
{
    line-height: normal !important;
    border: 0 !important;
    margin: 0 !important;
    padding: 0 !important;
}
.gaugeStatsLineNatHaz
{
    clear: left;
}
.gaugeStatsLabelNatHaz
{
    float: left;
    width: 210px;
}
.gaugeStatsNumberNatHaz
{
    float: left;
}
.gaugeStatsNatHazPracticeContainer
{
    padding-right: 50px;
    float: left;
}
.gaugeStatsLabelNatHazPractice
{
}
.gaugeStatsLineHeading
{
    width: 80%;
    text-align: center;
    font-size: 14px;
    font-weight: bold;
    padding-bottom: 10px;
    color: #94B2D6; /* #a2b5cc; #5b63b7; #C6D1DE; */
}


/*** OTHER ***/
.gaugeCompanyName
{
    font-size: 28px;
    color: #777777;
    font-weight: bold;
    text-align: center;
}
.gaugeTagLine
{
    text-align: center;
    padding: 0;
    margin: 0;
    line-height: 8pt;
}
.gaugeWelcome
{
    text-align: center;
    padding: 0;
    margin: 0;
    line-height: 6pt;
}
    

/***** END GAUGE STYLES ******/