.k-button
{
    /*** MAKE BUTTONS SMALLER ***/
    padding-right: 0px;
    padding-left: 0px;
    line-height: 1.0em;
}
.k-button-padding
{
    padding-right: 6px;
    padding-left: 6px;
    line-height: 2.0em;
}
.k-grid tbody .k-button
{
    /*** FIX MINIMUM WIDTH OF BUTTON ***/
    min-width: 38px;
}
