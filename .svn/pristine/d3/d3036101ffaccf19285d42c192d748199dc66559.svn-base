/*** FIX FOR ALLOWING THE GRID COLUMN HEADERS TO WRAP ***/
/*** AND CENTER THE COLUMN HEADER TEXT ***/
.k-grid  .k-grid-header .k-header  .k-link {
    height: auto;
    vertical-align: central;
    text-align: center;
    font-size: 7pt;
    line-height: 9pt;
    font-weight: normal;
    color: #FFFFFF; /*#404040;*/ /* #595959; */;
    border-bottom: none;
}
  
.k-grid  .k-grid-header .k-header {
    min-height: 28px;
    vertical-align: central;
    white-space: normal;
    text-align: center;
    font-size: 7pt;
    line-height: 9pt;
    font-weight: normal;
    color: #FFFFFF; /*#404040;*/ /* #595959; */;
}

.k-grid  .k-grid-header .k-header  .k-icon {
    float: right;
}

.k-grid-header .k-with-icon .k-link {
    margin-right: 2em;
}

.k-header .k-with-icon,
.k-header-column-menu {
    /*margin-top: 0 !important;*/
    vertical-align: middle !important;
    line-height: normal !important;
}

/*** ADD GRID HEADER BORDER ***/
.k-grid  .k-grid-header .k-header {
    /*border: 1px solid #CECFCE !important;*/
}


/*** CORRECTION FOR GRID HEADER MENU ICONS ***/
.k-header > .k-grid-finder,
.k-header > .k-header-column-menu {
    margin-right: -0.2em;
}


/*** CORRECTIONS FOR GRID ROWS ***/
.k-grid tbody tr {
    /*height: 32px !important;*/
    height: 18px !important;
}
.k-grid td {
    white-space: nowrap;
    text-align: left;
    color: #111111;
}
.k-grid thead .k-link {
    text-overflow: clip !important;
    font-weight: bold;
    color: #111111;
}
.k-grid thead .k-link:hover {
    text-decoration: underline;
}
.k-grid-pager {
    border: 1px solid #CECFCE !important;
}


/*** FIX UPPER RIGHT CORDER OF SCROLLING GRID ***/
.k-resize-handle {
    border-top: 1px solid #CECFCE;
}


/*** FIX BACKGROUND COLOR OF TEXTBOXES IN FILTER MENU POPUP ***/
.k-filter-menu .k-textbox {
    background-color: #FFFFFF;
}

/*COMBOBOXES ON BLUE HEADER*/
#topBannerNav .k-header
{
    /*** CHANGE BORDER COLOR FROM WHITE TO BLUE ***/
    background-color: #0046ad !important;
}


/*STB 12-19-2016*/

/*USE WHITE BACKGROUND*/
.k-grid-pager {
    background-color: #FFFFFF;
}
/*USE WHITE TEXT ON DARK BACKGROUNDS IN SPECIFIC AREAS*/
.k-pager-info {
    color: #000000; /*#FFFFFF;*/
    font-size: 8pt;
    font-weight: normal;
    padding-top: 2px;
}

/*CORRECT WHITE TEXT IN POPUP MENUES*/
/*.k-widget.k-reset.k-header.k-menu.k-menu-vertical {
    color: #000000;
}*/

/*USE DARK TEXT ON GRID NAV BUTTONS*/
.k-pager-nav {
    color: #FFFFFF; /*#000000;*/
}

/*CORRECT DISABLED GRID NAV BUTTONS*/
.k-grid-pager > a.k-state-disabled,
.k-grid-pager > a.k-state-disabled:hover {
    background-color: #CCCCCC;
    cursor: default;
}

.k-grid-pager > a,
.k-grid-pager .k-link {
    background:#0046ad;
    color:#fff;
    /*background-color: #FFFFFF;*/
    cursor: pointer;
    border-color: #DCDCDC;
    font-size: 8pt;
    font-weight: normal;
}
.k-grid-pager > a:hover,
.k-grid-pager .k-link:hover {
    background-color: #165597;
    color: #FFFFFF;
    cursor: pointer;
    font-size: 8pt;
    font-weight: normal;
}

/*FIX THE BACKGROUND IMAGE TO USE WHITE COLOR*/
.k-grid-pager .k-icon:hover {
    /*background-image: url('Kendo.v2014.1.318/Silver/sprite_White.png');
    background-image: url('sprite_white.png');*/
}

.k-grid-pager > .k-pager-refresh,
.k-grid-pager > .k-pager-refresh > a {
    background-color: transparent;
}

/*MAKE BUTTONS LOOK LIKE EDA WITH WHITE BACKGROUND*/
.k-pager-numbers > a,
.k-pager-numbers .k-link {
    border: none;
    font-size: 8pt;
    font-weight: normal;
    min-width: 2.2em;
    min-height: 2.0em;
    padding-top: 2px;
}
.k-pager-numbers > a:hover,
.k-pager-numbers .k-link:hover {
    border: none;
    background-image: none;
    font-size: 8pt;
    font-weight: normal;
    min-width: 2.2em;
    min-height: 2.0em;
    padding-top: 2px;
}
.k-pager-numbers .k-state-selected {
    border: none;
    background-color: #B2C7DF;
    color: #FFFFFF;
    font-size: 8pt;
    font-weight: normal;
    min-width: 2.2em;
    min-height: 2.0em;
    padding-top: 2px;
}

/* Tooltip for Grid */
.k-tooltip {
  border-color: #0070c0;
  background-color: #a8a8a8;
  color: #2e2e2e;
  text-align: left;
  font-size:12px;
}

.k-tooltip-button {
    display:none !important;
}

.k-tooltip-validation {
  border-color: #ffe79e;
  background-color: #fff4c9;
  color: #635145;
  text-align: left;
}

