/***** CUSTOM STYLE BY STB 20120426 *******/
.k-grouping-header
{
}
.k-grid 
{
    font-family: Arial;
    font-size: 7pt;
    /*** THIS TURNS SURROUNDING BORDER OFF ***/
    border: 0px;

}
.k-grid-header-wrap
{
    /*** FIX FOR BORDER IN HEADER FOR SCROLLBAR ***/
    border: 0px !important;
    border-bottom: none !important;
    border-bottom-width: 0px !important;
    border-bottom-color: transparent !important;
}
.k-grid thead
{
    border-bottom: 1px solid #CECFCE !important;
}
.k-grid table
{
    /*** COLLAPSE BORDERS IN CELLS TO ONE BORDER, NOT SEPARATE ***/
    border-collapse: collapse;
}
.k-grid-header .k-header:first-child, .k-grid tbody td:first-child, .k-grid tfoot td:first-child
{
    /*** LEFT MOST BORDER OF TABLE ***/
    border-left-width: 1px;
}
.k-grid td
{
    /*** SEPARATING BORDER BETWEEN ROWS ***/
    border-top: 1px solid #CECFCE;
    border-bottom: 1px solid #CECFCE;
    border-right: 1px solid #CECFCE;
    border-left: 1px solid #CECFCE;
}
.k-grid .k-detail-row td {
    /*** CLEARING OF ABOVE STYLE ON DETAIL POPUP ***/
    border-top: 1px solid #FFFFFF;
    border-bottom: 1px solid #FFFFFF;
    border-right: 1px solid #FFFFFF;
    border-left: 1px solid #FFFFFF;
}
.k-pager-wrap
{
    /*** FIX 1px BORDER FROM TBODY ***/
    border-right: 1px solid #0046ad;
}
.k-grid-header
{
    /*** FIX 1px BORDER FROM TBODY ***/
    border-right: 1px solid #0046ad;
    /*** FIX HEADER BOTTOM BORDER ***/
    border-bottom: none !important;
    border-bottom-width: 0px !important;
    border-bottom-color: transparent !important;
}
.k-grid-content
{
    /*** FIX FOR BORDER ON BOTTOM, RIGHT, AND LEFT OF TABLE ***/
    border-bottom: 1px solid #CECFCE;
    border-right: 1px solid #CECFCE;
    border-left: 1px solid #CECFCE;
    border-collapse: collapse;
}
.k-grid-header .k-header {
    overflow: hidden;
    border-style: solid;
    border-width: 0 0 0px 0px;
    padding: .5em .2em .4em .2em;
    font-weight: normal;
    white-space: nowrap;
    text-overflow: ellipsis;
    background-color: #0046ad; /*#0046ad; ORIGINAL*/ /*#D6DEE7; A LITTLE TOO BRIGHT*/ /*#d2dbe5 ALMOST RIGHT*/
    /*border-top: 1px solid #0046ad;*/
    border-left: 1px solid #0046ad;
    border-right: 1px solid #0046ad;
}
.k-pager-wrap
{
    /* CORRECTION OF COLOR AND BORDER OF DRAG HANDLE AND HEADING */
    background-color: #0046ad; /*#0046ad; ORIGINAL*/ /*#D6DEE7; A LITTLE TOO BRIGHT*/ /*#d2dbe5 ALMOST RIGHT*/
}
.k-grid-header,
.k-draghandle .k-header 
{
    /* CORRECTION OF COLOR AND BORDER OF DRAG HANDLE AND HEADING */
    background-color: #0046ad; /*#0046ad; ORIGINAL*/ /*#D6DEE7; A LITTLE TOO BRIGHT*/ /*#d2dbe5 ALMOST RIGHT*/
    border-top: 1px solid #CECFCE;
    border-collapse: collapse;
}

.k-state-selected,
.k-list > .k-state-selected,
.k-list > .k-state-focused
{
  color: #363636;
  /*background-color: #edeef0; #e9ecf0; /*#e0e7f0; /*#e1e4e7;*/
  background-image: none;
  border-color: #5b63b7;
}
