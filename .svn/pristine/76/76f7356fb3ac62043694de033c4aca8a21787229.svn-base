/* GLOBAL FOR ALL WINDOWS */
.windowIcon
{
    width:18px;
    height:18px;
    border:0;
    padding-right:10px;
}
.windowImage
{
    border: 0;
    width: 32px;
    height: 32px;
    padding-right: 20px;
    padding-bottom: 20px;
}
.windowImageSmallPadding
{
    border: 0;
    width: 32px;
    height: 32px;
    padding-right: 20px;
    padding-bottom: 10px;
}
.windowContent
{
    padding: 15px !important;
    display: none;
}
.windowSection
{
    background-color: #FFFFFF;
    border: 1px solid #8a8a8a;
    padding: 6px;
    width: 100%;
}
.windowSubHeading
{
    width: 100%;
    padding: 7px;
    margin: 0;
    background-color: #2A2A2A;
	color: #C6D1DE; 
 	background-image: url('../../Home_{{VER}}/Images/banners/topbanner_blankarea_small.png'); 
	background-position: top left;
	background-repeat: repeat-x;
	border-top: 0;
	border-right: 0;
	border-left: 0;
	font-weight: bold;
}
.windowSuperScript
{
    font-size: 10pt;
    font-style: italic;
    color: Red;
    padding-bottom: 4px;
}

