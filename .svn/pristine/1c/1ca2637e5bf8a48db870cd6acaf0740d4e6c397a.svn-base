{
    height: 24px;
    width: 100%;
}
.tableDiv div /** THIS IS A ROW **/
{
    clear: both;
    border-collapse: collapse;
    width: 100%;
}
.tableDiv span /** THIS IS A CELL **/
{
    /*float: left;*/
    border-collapse: collapse;
